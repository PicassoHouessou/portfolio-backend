{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "*",
        "beberlei/doctrineextensions": "^1.3",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "gedmo/doctrine-extensions": "^3.15",
        "gesdinet/jwt-refresh-token-bundle": "*",
        "halaxa/json-machine": "^1.1",
        "karser/karser-recaptcha3-bundle": "^0.1.21",
        "league/commonmark": "^2.1",
        "league/flysystem-bundle": "^3.3",
        "lexik/jwt-authentication-bundle": "*",
        "nelmio/cors-bundle": "^2.4",
        "nesbot/carbon": "^3.6.0",
        "phpdocumentor/reflection-docblock": "5.5.0",
        "sentry/sentry-symfony": "^5.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/ux-react": "^2.14",
        "symfony/validator": "7.0.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "7.0.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "twig/extra-bundle": "^3.3",
        "twig/markdown-extra": "^3.3",
        "vich/uploader-bundle": "^2.2",
        "webonyx/graphql-php": "^15.9"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "hautelook/alice-bundle": "^2.13",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.0.*"
        }
    }
}
